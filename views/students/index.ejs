<!DOCTYPE html>
<html lang="en">
<head>
    <meta charset="UTF-8">
    <meta name="viewport" content="width=device-width, initial-scale=1.0">
    <title>ExpressLab</title>
</head>
<body>
    <h1>Student names!</h1>
    <ul>
        <% students.forEach(function(s) { %>
            <li>
                <%= s.student  %>
                -
                <%= s.passing ? 'pass' : 'fail'  %>
            </li>
        <% }) %>
    </ul>
</body>
</html>